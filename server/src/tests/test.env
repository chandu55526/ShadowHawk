# Server Configuration
PORT=5002
NODE_ENV=test

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/shadowhawk-test

# Redis Configuration
REDIS_URL=redis://localhost:6379/1

# JWT Configuration
JWT_SECRET=test-secret-key-min-32-characters-long
JWT_EXPIRES_IN=1h

# Rate Limiting
RATE_LIMIT_WINDOW_MS=1000
RATE_LIMIT_MAX_REQUESTS=1000
STRICT_RATE_LIMIT_WINDOW_MS=1000
STRICT_RATE_LIMIT_MAX_REQUESTS=1000

# Security
CORS_ORIGIN=http://localhost:3000
HELMET_ENABLED=false
COMPRESSION_ENABLED=false

# Logging
LOG_LEVEL=silent
LOG_FILE=logs/test.log
ERROR_LOG_FILE=logs/test-error.log

# Monitoring
PROMETHEUS_ENABLED=false
METRICS_PORT=9091 